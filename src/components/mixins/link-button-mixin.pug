mixin link-button(linkText, link)
    a.link-button(href=`${link}`)
        | #{linkText} 
        svg.arrow(width="5" height="9" viewBox="0 0 5 9" fill="none" xmlns="http://www.w3.org/2000/svg")
            path(d="M1 1L4.53553 4.53553" stroke="#000000" stroke-width="0.8" stroke-linecap="round")
            path(d="M1 8.125L4.53553 4.58947" stroke="#000000" stroke-width="0.8" stroke-linecap="round")

